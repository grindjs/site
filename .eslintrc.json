{
  "plugins": ["eslint-plugin-import-auto-name", "prettier"],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": "error"
  },
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "globals": {
    "ValidationError": false
  },
  "extends": ["plugin:prettier/recommended"]
}
