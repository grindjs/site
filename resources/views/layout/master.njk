<!DOCTYPE html>
<html>
	<head>
		<title>{% block title %}Grind | Web Framework for Node.js{% endblock %}</title>
		<meta name="viewport" content="width=1070" />

		{{ html.style('https://s3.amazonaws.com/assets.grind.rocks/docs/css/BentonSans.css') }}
		{{ html.style('https://s3.amazonaws.com/assets.grind.rocks/docs/css/fira_code.css') }}

		{% asset 'render', 'styles' %}
		{% block styles %}{% endblock %}
	</head>
	<body>
		{% block contentBefore %}{% endblock %}

		<div class="container">
			<div class="content">
				{% block content %}{% endblock %}
			</div>
		</div>

		{% block contentAfter %}{% endblock %}

		{% asset 'render', 'scripts' %}
		{% block scripts %}{% endblock %}
	</body>
</html>
